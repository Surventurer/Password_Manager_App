<CreateAccountWindow>:
    name: "create"

    namee: namee
    email: email
    password: passw

    FloatLayout:
        cols:1

        FloatLayout:
            size: root.width, root.height/2

            Label:
                text: "Create an Account"
                size_hint: 0.8, 0.2
                pos_hint: {"x":0.1, "top":1}
                font_size: (root.width**2 + root.height**2) / 14**4

            Label:
                size_hint: 0.5,0.12
                pos_hint: {"x":0, "top":0.8}
                text: "Name: "
                font_size: (root.width**2 + root.height**2) / 14**4

            TextInput:
                pos_hint: {"x":0.5, "top":0.8}
                size_hint: 0.4, 0.12
                id: namee
                multiline: False
                font_size: (root.width**2 + root.height**2) / 14**4

            Label:
                size_hint: 0.5,0.12
                pos_hint: {"x":0, "top":0.8-0.13}
                text: "Email: "
                font_size: (root.width**2 + root.height**2) / 14**4

            TextInput:
                pos_hint: {"x":0.5, "top":0.8-0.13}
                size_hint: 0.4, 0.12
                id: email
                multiline: False
                font_size: (root.width**2 + root.height**2) / 14**4

            Label:
                size_hint: 0.5,0.12
                pos_hint: {"x":0, "top":0.8-0.13*2}
                text: "Password: "
                font_size: (root.width**2 + root.height**2) / 14**4

            TextInput:
                pos_hint: {"x":0.5, "top":0.8-0.13*2}
                size_hint: 0.4, 0.12
                id: passw
                multiline: False
                password: True
                font_size: (root.width**2 + root.height**2) / 14**4

        Button:
            pos_hint:{"x":0.3,"y":0.25}
            size_hint: 0.4, 0.1
            font_size: (root.width**2 + root.height**2) / 16**4
            text: "Already have an Account? Log In"
            on_release:
                root.manager.transition.direction = "left"
                root.login()

        Button:
            pos_hint:{"x":0.2,"y":0.05}
            size_hint: 0.6, 0.15
            text: "Submit"
            font_size: (root.width**2 + root.height**2) / 14**4
            on_release:
                root.manager.transition.direction = "left"
                root.submit()


<LoginWindow>:
    name: "login"

    email: email
    password: password
	FloatLayout:
        cols:1
		FloatLayout:
			size: root.width, root.height/2
			Label:
				text: "Log In"
				size_hint: 0.8, 0.2
				pos_hint: {"x":0.1, "top":1}
				font_size: (root.width**2 + root.height**2) / 14**4
			Label:
				text:"Email: "
				font_size: (root.width**2 + root.height**2) / 14**4
				pos_hint: {"x":0, "top":0.8}
				size_hint: 0.5, 0.12

			TextInput:
				id: email
				font_size: (root.width**2 + root.height**2) / 14**4
				multiline: False
				pos_hint: {"x": 0.5 , "top":0.8}
				size_hint: 0.4, 0.12

			Label:
				text:"Password: "
				font_size: (root.width**2 + root.height**2) / 14**4
				size_hint: 0.5,0.12
                pos_hint: {"x":0, "top":0.8-0.13}

			TextInput:
				id: password
				font_size: (root.width**2 + root.height**2) / 14**4
				multiline: False
				password: True
				pos_hint: {"x":0.5, "top":0.8-0.13}
                size_hint: 0.4, 0.12

        Button:
            pos_hint:{"x":0.3,"y":0.25}
            size_hint: 0.42, 0.1
            font_size: (root.width**2 + root.height**2) / 16**4
            text: "Don't have an Account? Create One"
            on_release:
                root.manager.transition.direction = "right"
                root.createBtn()
		Button:
            pos_hint:{"x":0.2,"y":0.05}
            size_hint: 0.6, 0.15
            font_size: (root.width**2 + root.height**2) / 14**4
            text: "Login"
            on_release:
                root.manager.transition.direction = "up"
                root.loginBtn()

<Introduction>:
	name: "Introduction"
    FloatLayout:
        cols:1

        FloatLayout:
			size: root.width, root.height/2
            Label:
                text: "Welcome To LOCKWORD,\n      Password Manager"
                size_hint: 0.8, 0.2
                pos_hint: {"x":0.1, "top":1}
                font_size: (root.width**2 + root.height**2) / 14**4

            Label:
                size_hint: 0.1,0.2
                pos_hint: {"x":0.45, "top":0.7}
                text: "We store your password with special and unique\n            encryption, which ensures Total Security"
                font_size: (root.width**2 + root.height**2) / 14**4

            Label:
                size_hint: 0.1,0.2
                pos_hint: {"x":0.47, "top":0.5-0.11}
                text: "- Developed By Abhinav Rajpati"
                font_size: (root.width**2 + root.height**2) / 14**4

        Button:
            pos_hint:{"x":0.70,"y":0.11}
            size_hint: 0.2, 0.1
            font_size: (root.width**2 + root.height**2) / 13.2**4
            text: "Next.."
            on_release:
                root.manager.transition.direction = "left"
                root.login()

<MainWindow>:
	main:"main"
    n: n
    email: email
    created:created

    FloatLayout:
        Label:
            id: n
            pos_hint:{"x": -0.23, "top":1.02}
            size_hint:0.8, 0.2
            text: "Account Name: "
			font_size: (root.width**2 + root.height**2) / 16.5**4

        Label:
            id: email
            pos_hint:{"x": 0.17,"top":1.02}
            size_hint:0.8, 0.2
            text: "Email: "
			font_size: (root.width**2 + root.height**2) / 16.5**4

        Label:
            id: created
            pos_hint:{"x": 0.48,"y":-0.03}
            size_hint:0.8, 0.2
            text: "Created: "
			font_size: (root.width**2 + root.height**2) / 16.5**4

        Button:
            pos_hint:{"x":0.78, "top": 0.97}
            size_hint:0.2,0.1
            text: "Log Out"
			font_size: (root.width**2 + root.height**2) / 15**4
            on_release:
                app.root.current = "login"
                root.manager.transition.direction = "down"
				
		Button:
            pos_hint:{"x":0.2,"y":0.6}
            size_hint: 0.6, 0.1
            font_size: (root.width**2 + root.height**2) / 14**4
            text: "Add a Password"
            on_release:
                root.manager.transition.direction = "left"
                app.root.current="Add"
		Button:
            pos_hint:{"x":0.2,"y":0.6-0.13}
            size_hint: 0.6, 0.1
            font_size: (root.width**2 + root.height**2) / 14**4
            text: "Lookup your stored Password"
            on_release:
                root.manager.transition.direction = "left"
                app.root.current = "look"
		Button:
            pos_hint:{"x":0.2,"y":0.6-0.13*2}
            size_hint: 0.6, 0.1
            font_size: (root.width**2 + root.height**2) / 14**4
            text: "Delete the Password"
            on_release:
                root.manager.transition.direction = "left"
                app.root.current = "delete"
		Button:
            pos_hint:{"x":0.02}
            size_hint:0.29,0.1
            text: "Delete the Account"
			font_size: (root.width**2 + root.height**2) / 15**4
            on_release: root.btn()

		
